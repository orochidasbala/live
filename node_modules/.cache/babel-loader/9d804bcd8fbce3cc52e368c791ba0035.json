{"ast":null,"code":"import \"core-js/modules/es.error.cause.js\";\nimport { ref } from \"@vue/reactivity\";\nimport { createUserWithEmailAndPassword, debugErrorMap, updateProfile } from \"@firebase/auth\";\nimport { auth } from \"@/firebase/config\";\nexport default {\n  setup() {\n    let username = ref(\"\");\n    let email = ref(\"\");\n    let password = ref(\"\");\n    let error = ref(null);\n\n    let signupNow = async () => {\n      try {\n        let res = await createUserWithEmailAndPassword(auth, email.value, password.value);\n\n        if (!res) {\n          throw new Error(\"can't create new account. something went wrong!\");\n        }\n\n        res.user.updateProfile({\n          displayName: username\n        });\n      } catch (err) {\n        error.value = err.message;\n      }\n    };\n\n    return {\n      username,\n      email,\n      password,\n      signupNow,\n      error\n    };\n  }\n\n};","map":{"version":3,"mappings":";AA+BA,SAASA,GAAT,QAAoB,iBAApB;AACA,SACIC,8BADJ,EAEIC,aAFJ,EAGIC,aAHJ,QAIO,gBAJP;AAKA,SAASC,IAAT,QAAqB,mBAArB;AACA,eAAe;EACXC,KAAK,GAAG;IACJ,IAAIC,QAAO,GAAIN,GAAG,CAAC,EAAD,CAAlB;IACA,IAAIO,KAAI,GAAIP,GAAG,CAAC,EAAD,CAAf;IACA,IAAIQ,QAAO,GAAIR,GAAG,CAAC,EAAD,CAAlB;IACA,IAAIS,KAAI,GAAIT,GAAG,CAAC,IAAD,CAAf;;IAEA,IAAIU,SAAQ,GAAI,YAAY;MACxB,IAAI;QACA,IAAIC,GAAE,GAAI,MAAMV,8BAA8B,CAC1CG,IAD0C,EAE1CG,KAAK,CAACK,KAFoC,EAG1CJ,QAAQ,CAACI,KAHiC,CAA9C;;QAKA,IAAI,CAACD,GAAL,EAAU;UACN,MAAM,IAAIE,KAAJ,CACF,iDADE,CAAN;QAGJ;;QACAF,GAAG,CAACG,IAAJ,CAASX,aAAT,CAAuB;UAAEY,WAAW,EAAET;QAAf,CAAvB;MACJ,CAZA,CAYE,OAAOU,GAAP,EAAY;QACVP,KAAK,CAACG,KAAN,GAAcI,GAAG,CAACC,OAAlB;MACJ;IACH,CAhBD;;IAkBA,OAAO;MAAEX,QAAF;MAAYC,KAAZ;MAAmBC,QAAnB;MAA6BE,SAA7B;MAAwCD;IAAxC,CAAP;EACH;;AA1BU,CAAf","names":["ref","createUserWithEmailAndPassword","debugErrorMap","updateProfile","auth","setup","username","email","password","error","signupNow","res","value","Error","user","displayName","err","message"],"sourceRoot":"","sources":["/home/dking/Desktop/live/src/components/Signup.vue"],"sourcesContent":["<template>\n    <form class=\"form\" @submit.prevent=\"signupNow\">\n        <h1>Sign Up</h1>\n        <input\n            type=\"text \"\n            placeholder=\"Username\"\n            class=\"form-input\"\n            v-model=\"username\"\n            required\n        />\n        <input\n            type=\"text\"\n            placeholder=\"Email\"\n            autocomplete=\"off\"\n            class=\"form-input\"\n            v-model=\"email\"\n            required\n        />\n        <input\n            type=\"password\"\n            placeholder=\"Password\"\n            class=\"form-input\"\n            v-model=\"password\"\n            required\n        />\n        <button>Create account</button>\n        <p v-if=\"error\">Error : {{ error }}</p>\n    </form>\n</template>\n\n<script>\nimport { ref } from \"@vue/reactivity\";\nimport {\n    createUserWithEmailAndPassword,\n    debugErrorMap,\n    updateProfile,\n} from \"@firebase/auth\";\nimport { auth } from \"@/firebase/config\";\nexport default {\n    setup() {\n        let username = ref(\"\");\n        let email = ref(\"\");\n        let password = ref(\"\");\n        let error = ref(null);\n\n        let signupNow = async () => {\n            try {\n                let res = await createUserWithEmailAndPassword(\n                    auth,\n                    email.value,\n                    password.value\n                );\n                if (!res) {\n                    throw new Error(\n                        \"can't create new account. something went wrong!\"\n                    );\n                }\n                res.user.updateProfile({ displayName: username });\n            } catch (err) {\n                error.value = err.message;\n            }\n        };\n\n        return { username, email, password, signupNow, error };\n    },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module"}